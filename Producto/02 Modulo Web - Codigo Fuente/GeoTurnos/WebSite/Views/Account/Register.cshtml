@model WebSite.Models.RegisterViewModel
@{
    ViewBag.Title = "Registro";
}

<h2>Elije un tipo de registro</h2><hr />

<div class="row">
    <div class="col-md-6">
        <h3>Entidad Prestadora</h3>
        <a href="/Account/registrarempresa">
            <img class="img-responsive img-thumbnail" src="~/resources/O9IY3N0.png" alt="Entidad Prestadora" style="width:50%;height:50%"/>
        </a>
    </div>
    <div class="col-md-6">
        <h3>Cliente Prestatario</h3>
        <a href="/Account/registrarcliente">
            <img class="img-responsive img-thumbnail" src="~/resources/O9IY170.png" alt="Cliente Prestatario" style="width:50%;height:50%" />
        </a>
    </div>
</div>



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
